  methods.md
 }
 lib=lib {
  sinatra.rb
  entity=rbbt/entity{
   structure.rb
  }
  structure=structure{
   annotator.rb
   ssw.rb
   alignment.rb

   pdb_helper.rb
   pdb_alignment.rb

   neighbours.rb

   interactome_3d.rb
   uniprot.rb
   interpro.rb
   COSMIC.rb
   appris.rb

   entity.rb
   sinatra.rb

   workflow=workflow{
    helpers.rb
    alignments.rb
    annotate.rb
    wizard.rb
   }
  }

 }
 share=share{
  rbbt_commands=rbbt_commands filter="*"{
   bootstrap
   sample_report
  }
  software=software/opt/ssw filter="*"{
   ssw.c
   ssw.h
  }
 }
 test=test{
  test_ssw.rb
  test_uniprot.rb
 }

